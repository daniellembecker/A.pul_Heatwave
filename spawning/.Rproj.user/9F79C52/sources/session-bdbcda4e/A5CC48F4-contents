  xlab(expression(bold(paste("Treatment"))))+
      scale_x_discrete(labels=c("ambient" = "Ambient", "hot" = "Hot")) +
    ggtitle("Kruskal-Wallis, p = 0.04") +
      annotate("text", x = 1.45, y = 100, label = "*", size = 20) +
